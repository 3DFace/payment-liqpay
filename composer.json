{
  "name": "dface/payment-liqpay",
  "description": "A PHP client library to work with Privatbank LiqPay API",
  "keywords": [
    "Payment",
    "LiqPay"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Ponomarev",
      "email": "ponomarev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "psr/log": ">=1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "dface/code-gen": "dev-master",
    "dface/tito": "dev-master",
    "roave/security-advisories": "dev-master",
    "guzzlehttp/guzzle": "^7.8",
    "dface/phpunit-customization": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "dface\\Payment\\LiqPay\\": [
        "lib/"
      ]
    },
    "psr-0": {
      "dface\\Payment\\LiqPay\\": [
        "gen/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "dface\\Payment\\LiqPay\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "8.2.5"
    }
  }
}
